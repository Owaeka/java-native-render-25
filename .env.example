# Server Configuration
SERVER_PORT=8080

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_SSL_ENABLED=true

# Keycloak Configuration
KEYCLOAK_URL=https://your-keycloak-instance.com
KEYCLOAK_ADMIN_REALM=master
KEYCLOAK_ADMIN_CLIENT_ID=your-admin-client-id
KEYCLOAK_ADMIN_CLIENT_SECRET=your-admin-client-secret

# Tenant Configuration
TENANT_1_KEY=your-tenant-api-key
TENANT_1_NAME=My First Project
TENANT_1_REALM=my-realm
TENANT_1_CLIENT_ID=my-client-id
TENANT_1_CLIENT_SECRET=my-client-secret

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:4200

# Rate Limiting
RATE_LIMIT_ENABLED=true

# Logging
LOG_LEVEL=DEBUG
